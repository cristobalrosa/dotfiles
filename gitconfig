[user]
    name = Your User Name
    email = your email
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
[core]
   editor=vim
   excludesfile = ~/.gitignore_global
[alias]
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    st = status
    ci = commit
    co = checkout
    df=diff
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    br = branch
    sb = show-branch
    staged = diff --cached
    cp = cherry-pick
[push]
    default = simple
